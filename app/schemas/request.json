{
    "type": "object",
    "required": ["settings", "future"],
    "settings":{
        "type": "object",
        "properties": {
            "growth": {
                "type": "string"
            },
            "changepoint_prior_scale": {
                "type": "integer",
                "minimum": 0
            },
            "seasonality_prior_scale": {
                "type": "integer",
                "minimum": 0
            },
            "interval_width": {
                "type": "number",
                "minimum": 0
            },
            "daily_seasonality": { "enum": [ false, true, "auto" ] },
            "weekly_seasonality": { "enum": [ false, true, "auto" ] },
            "yearly_seasonality": { "enum": [ false, true, "auto" ] },
            "seasonality": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {                
                        "name": {
                            "type": "string"
                        },
                        "period": {
                            "type": "number",
                            "minimum": 0
                        },
                        "fourier_order": {
                            "type": "integer",
                            "minimum": 0
                        }
                    }
                }       
            }
        }
    },
    "data": {
        "type": "array",
        "items": [
            {"type": "array"},
            {"items": [
                {"type": "number"},
                {"type": "number"},
                {"type": "number"}
                ]
            }
        ]
    },
    "regression": {
        "type": "array",
        "items": [
            {"type": "array"},
            {"items": [
                {"type": "number"},
                {"type": "number"},
                {"type": "number"}
                ]
            }
        ]
    }
}